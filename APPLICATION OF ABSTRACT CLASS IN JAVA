//INPUT
abstract class Animal{
  void legs(){
    System.out.println("Every animal has four legs");
  }
  abstract void sound();
}
class Cow extends Animal{
  @Override
  void sound(){
    System.out.println("Moooooo.....");
  }
}
public class AbsClass{
  public static void main(String[] args){
    Cow s = new Cow();
    s.sound();
    s.legs();
  }
}
//OUTPUT
Moooooo.....
Every animal has four legs
